{
    "type": "sqlite",
    "database": "database.sqlite.db",
    "synchronize": true,
    "logging": false,
    "entities": [
       "src/db/entity/**/*.ts"
    ],
    "migrations": [
       "src/migration/**/*.ts"
    ],
    "subscribers": [
       "src/subscriber/**/*.ts"
    ],
    "cli": {
       "entitiesDir": "src/db/entity",
       "migrationsDir": "src/migration",
       "subscribersDir": "src/subscriber"
    }
 }